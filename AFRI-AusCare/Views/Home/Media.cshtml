@model IEnumerable<AFRI_AusCare.Models.KeyPartner>

<!-- breadcam_area_start -->
<div class="breadcam_area bradcam_bg">
    <div class="container">
        <div class="row">
            <div class="col-xl-12">
                <div class="breadcam_text">
                    <h3>Media Page</h3>
                    <p class="text-white"> We are building a future where services will acknowledge and are sensitive to African and 
                        culturally and linguistically diverse (CALD) people´s perception of mental health.</p>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- breadcam_area_end -->
<!-- volunteers_area_satrt -->
<div class="volunteers_area">
    <div class="container">
        <div class="row">
            <div class="col-xl-12">
                <div class="section_title text-center mb-60">
                    <span>Below You Can See</span>
                    <h3>
                        Media Coverage Of Afri Aus Care Projects
                    </h3>
                </div>
            </div>
        </div>
        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-xl-3 col-md-6">
                    <div class="single_volunteer">
                        <div class="thumb text-center">
                            <img src="@item.ImageUrl" alt="" class="img-key-partner">
                            <div class="social_links">
                                <ul>
                                    <li><a href="@item.Facebook"><i class="fa fa-facebook"></i></a></li>
                                    <li><a href="@item.Twitter"><i class="fa fa-twitter"></i></a></li>
                                    <li><a href="@item.Instagram"><i class="fa fa-instagram"></i></a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</div>
<!-- volunteers_area_end -->
