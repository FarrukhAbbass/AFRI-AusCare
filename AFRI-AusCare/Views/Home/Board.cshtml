@model IEnumerable<AFRI_AusCare.Models.BoardMember>

<!-- breadcam_area_start -->
<div class="slider_area slider_bg_1 d-flex align-items-center">
    <div class="container">
        <div class="row">
            <div class="col-xl-12">
                <div class="single_slider">
                    <div class="slider_text">
                        <h3>Board Members</h3>
                        <p class="text-white">
                            Our dedicated board members work very hard behind the scenes to ensure Afri-Aus Care <br>
                            delivers quality services to the public. Thier endless experince and expertise play a key <br>
                            role in driving our passion to achieve our vision and setting the standard. With their <br>
                            continued support and encouragement we look forward to many years of contributing to the community.

                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- breadcam_area_end -->
<!-- volunteers_area_satrt -->
<div class="volunteers_area">
    <div class="container">
        <div class="row">
            <div class="col-xl-12">
                <div class="section_title text-center mb-60">
                    <span>We Work For</span>
                    <h3>
                        We Serve For Peoples
                    </h3>
                </div>
            </div>
        </div>
        <div class="row">
            @foreach (var item in Model)
            {

                <div class="col-xl-3 col-md-6">
                    <div class="single_volunteer">
                        <div class="thumb text-center">
                            <img src="@item.ImageUrl" alt="" class="img-key-partner">
                            <div class="social_links">
                                <ul>
                                    <li><a href="@item.Facebook"><i class="fa fa-facebook"></i></a></li>
                                    <li><a href="@item.Twitter"><i class="fa fa-twitter"></i></a></li>
                                    <li><a href="@item.Instagram"><i class="fa fa-instagram"></i></a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="author_name text-center">
                            <h3>@item.Name</h3>
                            <span>@item.Role</span>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</div>
<!-- volunteers_area_end -->
