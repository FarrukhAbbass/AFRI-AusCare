@model IEnumerable<AFRI_AusCare.Models.BoardMember>

<!-- breadcam_area_start -->
<div class="breadcam_area bradcam_bg">
    <div class="container">
        <div class="row">
            <div class="col-xl-12">
                <div class="breadcam_text">
                    <h3>Board Members</h3>
                    <p>
                        Our dedicated board members work very hard behind the scenes to ensure Afri-Aus Care delivers
                        quality services to the public. Thier endless experince and expertise play a key role in driving 
                        our passion to achieve our vision and setting the standard. With their continued support and encouragement
                        we look forward to many years of contributing to the community.

                    </p>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- breadcam_area_end -->

<!-- volunteers_area_satrt -->
<div class="volunteers_area">
    <div class="container">
        <div class="row">
            <div class="col-xl-12">
                <div class="section_title text-center mb-60">
                    <span>We Work For</span>
                    <h3>
                        We Serve For Peoples
                    </h3>
                </div>
            </div>
        </div>
        <div class="row">
            @foreach (var item in Model)
            {

                <div class="col-xl-3 col-md-6">
                    <div class="single_volunteer">
                        <div class="thumb">
                            <img src="@item.ImageUrl" alt="">
                            <div class="social_links">
                                <ul>
                                    <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                                    <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                                    <li><a href="#"><i class="fa fa-linkedin-square"></i></a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="author_name text-center">
                            <h3>@item.Name</h3>
                            <span>@item.Role</span>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</div>
<!-- volunteers_area_end -->
